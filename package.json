{
  "name": "@yuya-takeyama/replicate-docker-version-tag-action",
  "version": "1.0.0",
  "private": true,
  "description": "Replicate version tags for Docker images",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Yuya Takeyama",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.6.0",
    "@actions/github": "5.0.0"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "16.11.26",
    "@typescript-eslint/parser": "5.16.0",
    "@vercel/ncc": "0.33.3",
    "eslint": "8.11.0",
    "eslint-plugin-github": "4.3.6",
    "eslint-plugin-jest": "26.1.3",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.5.1",
    "jest-circus": "27.5.1",
    "js-yaml": "4.1.0",
    "prettier": "2.6.0",
    "ts-jest": "27.1.3",
    "typescript": "4.6.2"
  }
}
