{
  "name": "@yuya-takeyama/replicate-docker-version-tag-action",
  "version": "1.0.0",
  "private": true,
  "description": "Replicate version tags for Docker images",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Yuya Takeyama",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.5",
    "@actions/github": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.11.1",
    "@typescript-eslint/parser": "^4.1.1",
    "@vercel/ncc": "^0.23.0",
    "eslint": "^7.9.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^24.9.0",
    "jest-circus": "^26.4.2",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.2",
    "ts-jest": "^24.3.0",
    "typescript": "^4.0.3"
  }
